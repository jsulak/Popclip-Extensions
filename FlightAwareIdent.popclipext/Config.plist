<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Version</key>
	<integer>2</integer>
	<key>Extension Name</key>
	<string>FlightAware Ident</string>
	<key>Extension Identifier</key>
	<string>com.jamessulak.com.flightaware-ident</string>
	<key>Extension Description</key>
	<string>Open Ident in FlightAware</string>
	<key>Actions</key>
	<array>
		<dict>
			<key>URL</key>
			<string>https://sulak.flightaware.com/live/flight/{popclip text}</string>
			<!--<key>Image File</key>
			<string>TranslationIcon.png</string>-->
			<key>Title</key>
			<string>Ident</string>
			<key>Regular Expression</key>
			<string>^[a-zA-Z]{1,3}[0-9a-zA-Z]{2,7}$</string>
		</dict>
	</array>
</dict>
</plist>
